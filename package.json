{
  "name": "el-table-tree-workspace",
  "description": "el-table支持树拖拽",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run start:lib\" \"sleep 3 && npm run start:example\" --raw --kill-others",
    "start:lib": "pnpm -C packages/el-table-tree-dnd start",
    "start:example": "pnpm -C example start",
    "build:lib": "pnpm -C packages/el-table-tree-dnd build",
    "build:example": "pnpm -C example build"
  },
  "dependencies": {
    "el-table-tree-dnd": "workspace:^"
  },
  "keywords": [
    "el-table-tree-dnd",
    "el-table",
    "tree",
    "easy",
    "pragmatic-drag-and-drop"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "latest",
    "autoprefixer": "latest",
    "concurrently": "^8.2.2",
    "postcss": "latest",
    "vite": "latest",
    "vue-tsc": "latest"
  }
}
